prefix=/data/local/tmp/sysroot/usr
exec_prefix=${prefix}
includedir=${prefix}/include
libdir=${exec_prefix}/lib

Name: libidn2
Description: Library implementing IDNA2008 and TR46
Version: 2.3.7
Cflags: -I${includedir}
